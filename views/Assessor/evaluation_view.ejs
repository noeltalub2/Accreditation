                                                </a>
                                                <a href="/uploads/<%= application.certificate_file_local_dev %>"
                                                    class="btn btn-sm btn-primary" download>
                                                    <i class="ri-download-line"></i> Download
                                                </a>
                                        </td>
                                    </tr>
                                    <% } %>

                                        <!-- National -->
                                        <% if (application.certificate_file_national_dev) { %>
                                            <tr>
                                                <td>National</td>
                                                <td>
                                                    <%= application.organization_national_dev %>
                                                </td>
                                                <td>
                                                    <%= application.description_national_dev %>
                                                </td>
                                                <td>
                                                    <%= application.certificate_file_national_dev %>
                                                        <a href="/uploads/<%= application.certificate_file_national_dev %>"
                                                            class="btn btn-sm btn-primary">
                                                            <i class="ri-eye-line me-1"></i>View
                                                        </a>
                                                        <a href="/uploads/<%= application.certificate_file_national_dev %>"
                                                            class="btn btn-sm btn-primary" download>
                                                            <i class="ri-download-line"></i> Download
                                                        </a>
                                                </td>
                                            </tr>
                                            <% } %>

                                                <!-- International -->
                                                <% if (application.certificate_file_international_dev) { %>
                                                    <tr>
                                                        <td>International</td>
                                                        <td>
                                                            <%= application.organization_international_dev %>
                                                        </td>
                                                        <td>
                                                            <%= application.description_international_dev %>
                                                        </td>
                                                        <td>
                                                            <%= application.certificate_file_international_dev %>
                                                                <a href="/uploads/<%= application.certificate_file_international_dev %>"
                                                                    class="btn btn-sm btn-primary">
                                                                    <i class="ri-eye-line me-1"></i>View
                                                                </a>
                                                                <a href="/uploads/<%= application.certificate_file_international_dev %>"
                                                                    class="btn btn-sm btn-primary" download>
