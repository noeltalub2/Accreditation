                        <h6>B. Professional Development</h6>
                        <table class="table table-bordered">
                            <thead>
                                <tr>
                                    <th>Level</th>
                                    <th>Name of Organization/Scholarship</th>
                                    <th>Description</th>
                                    <th>Copy of Certificate</th>
                                </tr>
                            </thead>
                            <tbody>
                                <!-- Local/Regional -->
                                <% if (application.certificate_file_local_dev) { %>
                                    <tr>
                                        <td>Local/Regional</td>
                                        <td>
                                            <%= application.organization_local_dev %>
                                        </td>
                                        <td>
                                            <%= application.description_local_dev %>
                                        </td>
                                        <td>
                                            <%= application.certificate_file_local_dev %>
                                                <a href="/uploads/<%= application.certificate_file_local_dev %>"
